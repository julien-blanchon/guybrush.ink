{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": true,
      "previewPath": "/blog/",
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Slug",
          "name": "slug",
          "type": "string"
        },
        {
          "title": "Date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "Is it published",
          "name": "published",
          "type": "boolean"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "other",
  "frontMatter.framework.startCommand": "bun run dev",
  "frontMatter.git.enabled": true,
  "frontMatter.content.pageFolders": [
    {
      "title": "Blog",
      "path": "[[workspace]]/src/posts/"
    }
  ],
  "frontMatter.content.publicFolder": {
    "path": "src/assets",
    "relative": true
  },
  "frontMatter.content.supportedFileTypes": [
    "md"
  ],
  "frontMatter.preview.host": "http://localhost:5173",
  "frontMatter.preview.pathName": "blog",
  "frontMatter.site.baseURL": "https://guybrush.ink",
  "frontMatter.website.host": "https://guybrush.ink",
  "frontMatter.content.draftField": {
    "name": "published",
    "type": "boolean",
    "invert": true
  },
  "frontMatter.templates.enabled": true,
  "frontMatter.templates.folder": ".frontmatter/templates",
  "frontMatter.content.fmHighlight": true,
  "frontMatter.content.placeholders": [
    {
      "id": "author",
      "value": "Julien Blanchon"
    }
  ],
  "frontMatter.taxonomy.seoDescriptionLength": 160,
  "frontMatter.taxonomy.seoTitleLength": 75,
  "frontMatter.taxonomy.seoSlugLength": 75,
  "frontMatter.content.grouping": [
    {
      "title": "Date",
      "name": "date"
    },
    {
      "title": "Category",
      "name": "categories"
    },
    {
      "title": "Year",
      "name": "date|yyyy"
    }
  ]
}